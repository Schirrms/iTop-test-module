		<!-- many to may link between TestCI and TestChildCI needs an lnk type class -->
		<class id="lnkTestCIToTestChildCI" _delta="define">
			<parent>cmdbAbstractObject</parent>
			<properties>
				<is_link>1</is_link>
				<category>bizmodel</category>
				<abstract>false</abstract>
				<key_type>autoincrement</key_type>
				<db_table>lnktestcitotestchildci</db_table>
				<db_key_field>id</db_key_field>
				<db_final_class_field/>
				<naming>
					<attributes>
						<attribute id="testci_id"/>
						<attribute id="testchildci_id"/>
					</attributes>
				</naming>
				<display_template/>
				<reconciliation>
					<attributes>
						<attribute id="testci_id"/>
						<attribute id="testchildci_id"/>
					</attributes>
				</reconciliation>
			</properties>
			<fields>
				<field id="testci_id" xsi:type="AttributeExternalKey">
					<filter><![CDATA[SELECT TestCI WHERE location = :this->testchildci_location]]></filter>
					<dependencies>
						<attribute id="testchildci_location"/>
					</dependencies>
					<sql>testci_id</sql>
					<target_class>TestCI</target_class>
					<is_null_allowed>false</is_null_allowed>
					<on_target_delete>DEL_AUTO</on_target_delete>
				</field>
				<field id="testci_name" xsi:type="AttributeExternalField">
					<extkey_attcode>testci_id</extkey_attcode>
					<target_attcode>name</target_attcode>
				</field>
				<field id="testchildci_id" xsi:type="AttributeExternalKey">
					<sql>testchildci_id</sql>
					<target_class>TestChildCI</target_class>
					<is_null_allowed>false</is_null_allowed>
					<on_target_delete>DEL_AUTO</on_target_delete>
				</field>
				<field id="testchildci_name" xsi:type="AttributeExternalField">
					<extkey_attcode>testchildci_id</extkey_attcode>
					<target_attcode>name</target_attcode>
				</field>
				<field id="testchildci_location" xsi:type="AttributeExternalField">
					<extkey_attcode>testchildci_id</extkey_attcode>
					<target_attcode>location</target_attcode>
				</field>
			</fields>
			<presentation>
				<details>
					<items>
						<item id="testci_id">
							<rank>10</rank>
						</item>
						<item id="testchildci_id">
							<rank>20</rank>
